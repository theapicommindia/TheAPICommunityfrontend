{
  "redirects": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "postmancommunitypune.in"
        }
      ],
      "destination": "https://www.postmancommunitypune.in/",
      "permanent": true
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ],
  "env": {
    "VITE_BACKEND_URL": "https://the-api-communitybackend.vercel.app"
  }
}
